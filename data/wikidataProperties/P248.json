t"},"type":"statement","id":"P248$673c7409-487b-b32e-1e44-62a0209baa03","rank":"normal"},{"mainsnak":{"snaktype":"value","property":"P2559","datavalue":{"value":{"text":"Wykorzystanie tej w\u0142a\u015bciwo\u015bci wskazuje, \u017ce informacja jest zawarta w podanym \u017ar\u00f3dle (ksi\u0105\u017cka, artyku\u0142, itp.). Je\u015bli chcesz wskaza\u0107, \u017ce informacja pochodzi z innego elementu Wikidanych u\u017cyj P3452.","language":"pl"},"type":"monolingualtext"},"datatype":"monolingualtext"},"type":"statement","id":"P248$d9e501d3-4922-e5f9-d157-df99df7188c3","rank":"normal"}]}}}}